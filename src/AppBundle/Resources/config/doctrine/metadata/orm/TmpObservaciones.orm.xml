      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="productor" type="string" column="productor" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="presentado" type="string" column="presentado" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="certificado" type="string" column="certificado" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="inspeccionado" type="string" column="inspeccionado" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="registrado" type="string" column="registrado" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="especie" type="string" column="especie" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="actividad" type="string" column="actividad" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="tipo" type="string" column="tipo" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="observaciones" type="text" column="observaciones" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="dni" type="string" column="dni" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="cuit" type="string" column="cuit" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
