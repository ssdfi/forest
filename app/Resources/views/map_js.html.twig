{% set params = app.request.attributes.get('_route') %}
{% block javascripts %}
  <script src="{{ asset('js/jquery-3.1.1.js') }}"></script>
  {% if params == 'plantacionesaportes_show' %}
    <script src="{{ asset('js/maps/map.js') }}"></script>
  {% elseif params == 'plantacionesaportes_edit' %}
    <script src="{{ asset('js/maps/map_edit.js') }}"></script>
  {% else %}
    <script src="{{ asset('js/maps/map.js') }}"></script>
  {% endif %}
  <script src="{{ asset('js/leaflet.js') }}"></script>
  <script src="{{ asset('js/togeojson.js') }}"></script>
  <script src="{{ asset('js/Google.js') }}"></script>
  <script src="{{ asset('js/L.Control.ZoomBox.js') }}"></script>
  <script src="{{ asset('js/Control.MiniMap.js') }}"></script>
  <script src="{{ asset('js/leaflet.label.js') }}"></script>
  <script src="{{ asset('js/leaflet.filelayer.js') }}"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/leaflet.draw/0.4.2/leaflet.draw.js"></script>
  <script src="https://maps.google.com/maps/api/js?v=3&sensor=false&key={{google_maps_apikey}}" style=""></script>
{% endblock %}
