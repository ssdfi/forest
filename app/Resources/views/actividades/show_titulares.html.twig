{% set params = app.request.attributes.get('_route_params') %}
<div id="titulares-tab" class="tab-pane" role="tabpanel">
  <table id="titulares" class="table table-hover table-grid">
    <thead>
      <tr>
        <th>Titular</th>
        <th>Especie</th>
        <th>Tipo de Plantaci√≥n</th>
        <th>Presentado</th>
        <th>Certificado</th>
        <th>Inspeccionado</th>
        <th>Observaciones</th>
      </tr>
    </thead>
    <tbody>
      {% for productor in actividad.getActividadesTitulares() %}
        <tr>
          <td>

            <a href="/titulares/{{productor.titular.id}}">{{productor.titular.getNombre}}</a>
          </td>
          <td>{{productor.especie.codigoSp}}</td>
          <td>{{productor.tipoPlantacion.codigo}}</td>
          <td>{{productor.superficiePresentada}}</td>
          <td>{{productor.superficieCertificada}}</td>
          <td>{{productor.superficieInspeccionada}}</td>
          <td>{{productor.observaciones}}</td>
          <td><a id="nav-edit-productor" href="{{path('actividadestitulares_edit', {'idExp': params.id, 'idMov': params.idMov, 'idAct': params.idAct, 'id': productor.id})}}"><span class="glyphicon glyphicon-edit" aria-hidden="true"></a></span></td>
        </tr>
      {% endfor %}
    </tbody>
  </table>
</div>
